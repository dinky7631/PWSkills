   ],
   "source": [
    "def check_divisibility(A, B):\n",
    "    count = 0\n",
    "    while A % B == 0:\n",
    "        A //= B\n",
    "        count += 1\n",
    "    return count\n",
    "\n",
    "A, B = 64, 2\n",
    "print(f\"{A} is divisible by {B}, {check_divisibility(A, B)} times\")\n"
   ]
